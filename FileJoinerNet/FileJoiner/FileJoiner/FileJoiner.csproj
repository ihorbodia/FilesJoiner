    <ApplicationIcon>document.ico</ApplicationIcon>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Assets\document.ico" />
    <None Remove="document.ico" />
  </ItemGroup>
  <ItemGroup>
    <AvaloniaResource Include="Assets\document.ico">
